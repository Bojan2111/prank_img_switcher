{
  "manifest_version": 3,
  "name": "Image Prank Extension",
  "version": "1.0",
  "description": "Replace a specific image on a website with a custom one.",
  "host_permissions": ["*://specific-website.com/*"],
  "content_scripts": [
    {
      "matches": ["*://specific-website.com/*"],
      "js": ["content.js"]
    }
  ]
}
