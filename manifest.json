{
  "manifest_version": 3,
  "name": "Image Prank Extension",
  "version": "1.0",
  "description": "Replace a specific image on a website with a custom one.",
  "host_permissions": ["https://[URL:PORT || website.com || *.website.com]/*"],
  "content_scripts": [
    {
      "matches": ["https://[URL:PORT || website.com || *.website.com]/*"],
      "js": ["content.js"]
    }
  ]
}
